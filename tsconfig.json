{
    "compilerOptions": {
      "target": "es2016",                                 
      "module": "commonjs",                                
      "sourceMap": true,                               
      "esModuleInterop": true,                             
      "forceConsistentCasingInFileNames": true,            
      "strict": false,
      "noImplicitAny": true,
      "strictNullChecks": true,                         
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "skipLibCheck": true,
      "lib": [
        "ESNext",
        "DOM",
      ]
    }
  }